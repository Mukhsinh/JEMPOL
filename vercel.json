{
  "version": 2,
  "buildCommand": "npm install && npm run build --workspace=frontend",
  "outputDirectory": "frontend/dist",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-url.vercel.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
