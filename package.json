{
  "name": "kiss-root",
  "version": "1.0.0",
  "private": true,
  "description": "KISS - Keluhan Informasi Saran Sistem",
  "engines": {
    "node": "20.x"
  },
  "packageManager": "npm@10.2.4",
  "scripts": {
    "postinstall": "cd kiss && npm install --legacy-peer-deps",
    "install:all": "cd kiss && npm install --legacy-peer-deps",
    "dev": "cd kiss && npm run dev",
    "build": "cd kiss && npm run build",
    "preview": "cd kiss && npm run preview",
    "test": "cd kiss && npm test",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel"
  }
}
